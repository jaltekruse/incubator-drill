{
  head:{
    type:"APACHE_DRILL_LOGICAL",
    version:"1",
    generator:{
      type:"manual",
      info:"na"
    }
  },
  storage:{
    "dfs" :
      {
        "type":"file",
        "connection" : "file:///"
      }
  },
  query:[
    {
      @id:"1",
      op:"scan",
      memo:"initial_scan",
      storageengine:"dfs",
      selection: {
        format: {type: "parquet"},
        files: ["/tmp/test.parquet"]
      }
    },
    {
      @id:"2",
      input: 1,
      op: "project",
      projections: [
        { ref: "output.key", expr: "5" },
        { ref: "output.value", expr: "5+3" }
      ]
    },
    {
      @id:"3",
      input: 2,
      op: "store",
      memo: "output sink",
      target: {
        file: "console:///stdout"
      }

    }

  ]
}